<h2>Edit Account</h2>

<form method="POST" action="/edit/<%= name %>/?_method=PUT">
    <label for="userName">Name</label>
    <input type="text" name="userName" id="userName" value="<%= name %>">
    <label for="userEmail">Email</label>
    <input type="text" name="userEmail" id="userEmail" value="<%= email %>">
    <input type="submit">
</form>

